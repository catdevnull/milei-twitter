  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "benchmark:lastWeek": "node --enable-source-maps --import 'data:text/javascript,import {register} from \"node:module\"; import {pathToFileURL} from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"))' ./benchmarks/lastWeek.ts",
    "db:generate": "drizzle-kit generate",
    "db:reset-to-prod": "rm -f sqlite.db sqlite.db-shm sqlite.db-wal && sqlite3 sqlite.db 'PRAGMA journal_mode=WAL;' && turso db shell http://milei-prod:8080 '.dump' | sqlite3 sqlite.db",
    "db:seed-historic-likes": "node --enable-source-maps --import 'data:text/javascript,import {register} from \"node:module\"; import {pathToFileURL} from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"))' ./src/lib/db/seedHistoricLikes.ts"
  },
